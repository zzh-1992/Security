docker run \
--name zookeeper \
-p 2181:2181 \
-v /data/zookeeper/conf/zoo.cfg:/zookeeper-3.5.4/conf/zoo.cfg \
-v /data/zookeeper/data/:/data \
-v /data/zookeeper/datalog/:/datalog \
-v /data/zookeeper/logs/:/logs \
-d docker.io/zookeeper:latest

//docker run -p 2181:2181 --network host -v /usr/zookeeper/data:/data -v /usr/zookeeper/conf/zoo.cfg:/conf/zoo.cfg --name zookeeper -d docker.io/zookeeper

